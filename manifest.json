
{
    
     "update_url"
    : "https://clients2.google.com/service/update2/crx",
    "manifest_version"
    : 3,
    "name"
    : "COSIGN - ASL Translator",
    "short_name"
    : "COSIGN",
    "description"
    : "Translate Sign Language Realtime on Google Meet!",
    "version"
    : "1.0",
    "icons"
    : {
    "128"
    : "notmuted.png"
    },
    "host_permissions": ["http://localhost/*"],
    
    "content_scripts"
    : [{
    "matches"
    : ["https://meet.google.com/*"],
    "run_at"
    : "document_start",
    "js"
    : ["meet_controller.js"]
    }],
    
    "background"
    : {
    "service_worker": "keyboard_listener.js"
    },
    
    "permissions"
    : [
    "tabs",
    "notifications"
    ],
    "action": {
        "default_title": "Default Title",
        "default_popup": "popup.html"
      },
    
    "commands"
    : {
    "toggle"
    : {
    "description"
    : "Toggle mute",
    "global"
    : true,
    "suggested_key"
    : {
    "default"
    : "Ctrl+Shift+T",
    "mac"
    : "MacCtrl+Shift+T"
    }
    }
    }
    }